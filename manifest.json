{
    "manifest_version": 1,
    "name": "scrolly",
    "version": "1.0.0",

    "content_scripts": [
        {
            "matches": ["<all_urls>" ].
            "js": ["/scrolly.js" ]
        }
    ],

    "browser_action" {
        "browser_style": true,
        "default_icon": {
        "48": "/icon.svg"
        },
        "default_title": "scrolly",
        "default_popup": "/popup.html"
    },

    "permissions": [
        "storage"
    ]
}
